{
  "name": "@giffin/server",
  "version": "0.1.0",
  "author": "OllysCoding",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nodemon -e .ts \"build && start\"",
    "build": "rimraf ./dist && swc ./src/ -d ./dist --config-file .swcrc",
    "start": "node ./dist/src/index.js",
    "lint": "eslint --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "next": "16.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.1.2"
    },
  "devDependencies": {
    "@giffin/eslint-config": "workspace:*",
    "@giffin/typescript-config": "workspace:*",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "typescript": "5.9.2"
  }
}
